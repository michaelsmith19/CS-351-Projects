package deprecated;

import java.awt.Color;

import com.putable.tilenet.matrixelement.Image;

public abstract class XXXSetTagImpl implements XXXSetTag {
	private int x, y;				
	private String name;			
	private Color bgcolor, fgcolor;				
	private int tileWidth, tileHeight, energy;  
	private String text;						
	private Image image;

	@Override
	public int getX() {
		return x;
	}

	@Override
	public int getY() {
		return y;
	}

	@Override
	public Color getBgcolor() {		
		return bgcolor;
	}

	@Override
