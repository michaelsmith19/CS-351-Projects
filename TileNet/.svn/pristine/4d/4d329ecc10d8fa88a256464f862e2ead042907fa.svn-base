package com.putable.tilenet.factory;

public abstract class MatrixElement {
	private static long uniqueIdCounter;
	
	private String name;	
	private String objid;
	
	//Parts of MatrixElement
	
	SetTag tag;
	ElemType type;
	
	//Free stuff
	public String getName() { return name; }
	public void setName(String name) { this.name = name; }
	public String getObjid(){ return objid; }
	
	
	//Is set for all MatrixElements after creation in the builder
	public void setId(){		
		this.objid = type.getPrefix() + uniqueIdCounter++;
	}
	
	
	abstract void makeElement();
	
	
	

}
