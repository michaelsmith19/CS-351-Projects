package com.putable.tilenet.matrix;

import com.putable.tilenet.matrix.MatrixElementFactory.ElemType;

public abstract class MatrixElement {
	private static long uniqueObjID = 0;
	private long objid;
	
	public MatrixElement(ElemType type){
		++uniqueObjID;
		this.objid = uniqueObjID;
	}
	
	public long getid(){
		return this.objid;
	}

}
