package com.putable.tilenet.factory;

import java.awt.Color;

import com.putable.tilenet.matrix.Image;

public abstract class SetTagImpl implements SetTag {
	private int x, y;				
	private String name;			
	private Color bgcolor, fgcolor;				
	private int tileWidth, tileHeight, energy;  
	private String text;						
	private Image image;

	@Override
	public int getX() {
		return x;
	}

	@Override
	public int getY() {
		return y;
	}

	@Override
	public Color getBgcolor() {		
		return bgcolor;
	}

	@Override
	public Color getFgcolor() {
		return fgcolor;
	}

	@Override
	public int getTileWidth() {
		return tileWidth;
	}

	@Override
	public int getTileHeight() {
		return tileHeight;
	}

	@Override
	public int getEnergy() {
		return energy;
	}

	@Override
	public String getText() {
		return text;
	}

	@Override
	public Image getImage() {
		return image;
	}
	
	@Override
	public String getName(){
		return name;
	}

}
