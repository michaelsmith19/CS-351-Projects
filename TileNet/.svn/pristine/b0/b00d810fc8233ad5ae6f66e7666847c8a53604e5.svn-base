package com.putable.tilenet.factory;

import com.putable.tilenet.connection.AgentConnection;
import com.putable.tilenet.connection.Connection;
import com.putable.tilenet.matrixelement.ElemType;
import com.putable.tilenet.settag.AgentSetTag;
import com.putable.tilenet.settag.SetTag;

public class AgentFactory implements MatrixElementFactory {

	@Override
	public SetTag addSetTag() {
		return new AgentSetTag();
	}

	@Override
	public ElemType addElemType() {
		return ElemType.AGENT;
	}

	@Override
	public Connection addConnection() {
		return new AgentConnection();
	}

}
